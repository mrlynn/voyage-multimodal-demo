{
  "functions": {
    "app/api/upload/route.ts": {
      "maxDuration": 60
    },
    "app/api/upload-progress/route.ts": {
      "maxDuration": 300
    },
    "app/api/chat/route.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/uploads/:path*",
      "destination": "/api/static/:path*"
    }
  ]
}